
{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["dom", "esnext"],
    "downlevelIteration": true,
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "esnext",
    "isolatedModules": true,
    "skipLibCheck": true
  }
}
